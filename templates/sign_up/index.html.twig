{% extends 'base.html.twig' %}

{% block title %}Hello SignUpController!{% endblock %}


{% block navigation %}{% endblock %}

{% block body %}
<style>
    .example-wrapper {
        margin: 1em auto;
        max-width: 800px;
        width: 95%;
        font: 18px/1.5 sans-serif;
    }

    .example-wrapper code {
        background: #F5F5F5;
        padding: 2px 6px;
    }
</style>
<div id="root"></div>
{{form_start(form, {'attr':{'id':'signup_form'}})}}
<div id="q-1">
    {{form_row(form.meet)}}
</div>
<div id="q-2">
    {{form_row(form.Gender)}}
</div>
<div id="q-3">
    {{form_row(form.city)}}
    <a class="btn-floating  waves-effect waves-light red"><i class="material-icons">chevron_right</i></a>
</div>
<div id="q-4">
    {{form_row(form.firstname)}}
</div>
<div id="q-5">
    {{form_row(form.email)}}
</div>
<div id="q-7">
    {{form_row(form.password)}}
</div>
<div id="q-8">
    {{form_row(form.birthday)}}
</div>
<div id="q-9">
    {{form_row(form.isSerious)}}
</div>
<div id="q-10">
    {{form_row(form.isMarried)}}
</div>
<div id="q-11">
    {{form_row(form.hasChildren)}}
</div>
<div id="q-12">
    {{form_row(form.isSmoker)}}
</div>
{{form_end(form)}}
{% endblock %}